{
  "version": 2,
  "builds": [
    {
      "src": "student-counseling-app/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "student-counseling-app/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "student-counseling-app/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "student-counseling-app/public/$1"
    }
  ]
}
